pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
    

        stage('Build') {
            steps {
                echo 'Building'
            }
        }
    

        stage('Deploy') {
            steps {
                echo 'Depliying'
            }
        }
    
    

        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    }
}
